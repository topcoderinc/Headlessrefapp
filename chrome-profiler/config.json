{
  "routes": [
    {
      "url": "http://localhost:9000/",
      "selector": ".api-info-wrapper h1"
    },
    {
      "url": "http://localhost:9000/#section/Authentication",
      "selector": ".api-info-wrapper h1"
    },
    {
      "url": "http://localhost:9000/?url=https://api.apis.guru/v2/specs/instagram.com/1.0.0/swagger.yaml#tag/locations",
      "selector": ".api-info-wrapper h1"
    },
    {
      "url": "http://localhost:9000/?url=https://api.apis.guru/v2/specs/googleapis.com/calendar/v3/swagger.yaml#tag/channels",
      "selector": ".api-info-wrapper h1"
    },
    {
      "url": "http://localhost:9000/?url=https://api.apis.guru/v2/specs/data2crm.com/1/swagger.yaml#tag/Account",
      "selector": ".api-info-wrapper h1"
    },
    {
      "url": "http://localhost:9000/?url=https://api.apis.guru/v2/specs/graphhopper.com/1.0/swagger.yaml",
      "selector": ".api-info-wrapper h1"
    }
  ],
  "waitTimeout": 5000,
  "pageTimeout": 20000,
  "testBetweenTimeout": 500,
  "usageInterval": 1000,
  "profileFolder": "profiling-data",
  "chromePaths":["/usr/bin/google-chrome-unstable","/opt/google/chrome/chrome","C:/Program Files (x86)/Google/Chrome/Application/chrome.exe"],
  "puppeteerOptions": {
    "executablePath": "/usr/bin/google-chrome-stable",
    "args": [
      "--no-sandbox",
      "--disable-setuid-sandbox",
      "--disable-gpu",
      "--remote-debugging-address=0.0.0.0",
      "--remote-debugging-port=9223"
    ]
  },
  "tracingOptions": {
    "path": "trace.json",
    "categories": [
      "devtools.timeline"
    ]
  }
}
