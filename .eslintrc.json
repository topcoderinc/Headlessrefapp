{
  "extends": "airbnb-base",
  "env": {
      "node": true,
      "browser": true
  },
  "globals": {
    "angular": true
  },
  "rules": {
    "no-param-reassign": 0,
    "import/no-dynamic-require": 0,
    "no-loop-func": 0,
    "prefer-destructuring": 0,
    "no-console": 0,
    "no-await-in-loop": 0,
    "no-underscore-dangle": 0,
    "no-restricted-globals": 0,
    "no-mixed-operators": 0,
    "prefer-rest-params": 0
  }
}